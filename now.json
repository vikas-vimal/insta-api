{
	"version":2,
	"builds":[
		{
			"src":"index.js",
			"use":"@now/node-server"
		}
	],
	"routes":[
		{
			"src": "/",
			"dest": "/index.js",
			"methods": ["GET","POST"]
		},
		{
			"src": "/profile",
			"dest": "/index.js",
			"methods": ["POST"]
		},
		{
			"src": "/posts",
			"dest": "/index.js",
			"methods": ["POST"]
		}
	]
}